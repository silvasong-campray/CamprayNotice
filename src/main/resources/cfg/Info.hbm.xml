<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.campraynotice.dto">
	<class name="Info">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="title"></property>
		
		<property name="content" length="2000"></property>
		
		<property name="createTime" type="java.lang.Long"></property>
	</class>

</hibernate-mapping>